        //新闻列表-右侧24小时推荐排行
        CommonLoadNews("/NewsAjax/GetHotByDiggCount", "HotByDiggCountId", 7);
        //新闻列表-右侧热门IT新闻
        CommonLoadNews("/NewsAjax/GetRecentHotNews", "RecentHotNewsId", 10);
        //新闻列表-右侧最新评论
        LoadSideComments("NewsIndexCommentsId", 15);
        CheckAudit();
        LoadIndexSideInfo();
    });
</script>
